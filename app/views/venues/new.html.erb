<div class="row">
  <div class="col-12">
  <h1>Make a new Venue</h1>
  </div>
</div>


<div class="flex-container">

  <div class="block-container">
    <%= form_for(@venue) do |f| %>
        <h4><%= f.label :name %> </h4>
        <%= f.text_field :name  %> 

        <h4><%= f.label :address %></h4>
        <%= f.text_field :address %>
        <br><br>
        <%= f.submit %>
    <% end %>
</div>


 <% flash.each do |name, messages| %>
     <div class="flash-warnings">    
       <% messages.each do |message| %>
        <h4>  <%= message %> </h4>
       <% end %>
      </div>
      <% end %>